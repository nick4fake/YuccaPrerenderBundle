{
    "name": "nick4fake/prerender-bundle",
    "type": "symfony-bundle",
    "description": "Allows you to easily use prerender for your Symfony2 apps running Client-Side MVC",
    "keywords": ["prerender","MVC js"],
    "homepage": "http://rjanot.github.io/",
    "license": "MIT",
    "authors": [
        {
            "name": "RÃ©mi JANOT",
            "email": "r.janot@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "symfony/framework-bundle": "~2.3|~3.0",
        "psr/log": "1.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~3.7",
        "squizlabs/php_codesniffer": "1.4.*",
        "satooshi/php-coveralls": "~0.6"
